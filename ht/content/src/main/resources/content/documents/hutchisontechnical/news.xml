        <sv:property sv:name="ht:title" sv:type="String">
          <sv:value>Toyota suggests putting a glass of water on your dash (well, not really)</sv:value>
        </sv:property>
        <sv:node sv:name="hippotranslation:translations">
          <sv:property sv:name="jcr:primaryType" sv:type="Name">
